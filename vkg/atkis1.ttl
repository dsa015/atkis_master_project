@prefix : <http://example.org/ontologies/atkis#> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix gml: <http://www.opengis.net/ont/gml#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@base <http://example.org/ontologies/atkis#> .

<http://example.org/ontologies/atkis#> rdf:type owl:Ontology ;
                                        owl:imports <http://www.opengis.net/ont/gml> ,
                                                    <http://www.opengis.net/ont/geosparql/1.1> ,
                                                    <http://www.opengis.net/ont/sf/1.1> ,
                                                    rdfs: .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
wgs:alt rdf:type owl:AnnotationProperty ;
        rdfs:domain wgs:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
wgs:lat rdf:type owl:AnnotationProperty ;
        rdfs:domain wgs:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
wgs:location rdf:type owl:AnnotationProperty ;
             rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/based_near> ;
             rdfs:range wgs:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
wgs:long rdf:type owl:AnnotationProperty ;
         rdfs:domain wgs:SpatialThing .


###  http://xmlns.com/foaf/0.1/based_near
<http://xmlns.com/foaf/0.1/based_near> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/ontologies/atkis#existsIn
:existsIn rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#hasArea
:hasArea rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#hasRoad
:hasRoad rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#hasTraffic
:hasTraffic rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#hatDirektUnderpass
:hatDirektUnderpass rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#intersectsWith
:intersectsWith rdf:type owl:ObjectProperty ;
                rdfs:comment "land, water, location that intersects within another location" .


###  http://example.org/ontologies/atkis#isBuildingType
:isBuildingType rdf:type owl:ObjectProperty .


###  http://example.org/ontologies/atkis#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:comment "Is Located In"@en .


###  http://www.opengis.net/ont/geosparql#hasGeometry
geo:hasGeometry rdf:type owl:ObjectProperty .


###  http://www.opengis.net/ont/geosparql#hasLinestringGeometry
geo:hasLinestringGeometry rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf geo:hasGeometry .


###  http://www.opengis.net/ont/geosparql#hasPointGeometry
geo:hasPointGeometry rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf geo:hasGeometry .


###  http://www.opengis.net/ont/geosparql#hasPolygonGeometry
geo:hasPolygonGeometry rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf geo:hasGeometry .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
wgs:location rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://example.org/ontologies/atkis#area
:area rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:range xsd:string .


###  http://example.org/ontologies/atkis#beginn
:beginn rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:comment "Lifetime interval begins" .


###  http://example.org/ontologies/atkis#bemerkung
:bemerkung rdf:type owl:DatatypeProperty ;
           rdfs:comment "remark or comment" .


###  http://example.org/ontologies/atkis#bez
:bez rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Designation" ,
                  "Traffic code, highway code. Example E6" .


###  http://example.org/ontologies/atkis#bound
:bound rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#brf
:brf rdf:type owl:DatatypeProperty ;
     rdfs:comment "Width of roadway" .


###  http://example.org/ontologies/atkis#buildingPolygon
:buildingPolygon rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:comment "multipolygon border for buildings (sports, museums etc)" .


###  http://example.org/ontologies/atkis#bvb
:bvb rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Special traffic importance" .


###  http://example.org/ontologies/atkis#district
:district rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:string .


###  http://example.org/ontologies/atkis#ende
:ende rdf:type owl:DatatypeProperty ;
      rdfs:comment "Lifetime interval ends" .


###  http://example.org/ontologies/atkis#fkt
:fkt rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#fsz
:fsz rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Number of lanes" .


###  http://example.org/ontologies/atkis#ftr
:ftr rdf:type owl:DatatypeProperty ;
     rdfs:comment "Lane seperation" .


###  http://example.org/ontologies/atkis#gfk
:gfk rdf:type owl:DatatypeProperty ;
     rdfs:comment """categorisation of buildings defined by gfk values.
e.g. gfk 2072 = youth hostel"""@en .


###  http://example.org/ontologies/atkis#hdu_x
:hdu_x rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:comment "Has direct under pass" .


###  http://example.org/ontologies/atkis#idb
:idb rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "International importance" .


###  http://example.org/ontologies/atkis#land
:land rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:comment "Country code" .


###  http://example.org/ontologies/atkis#lat
:lat rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:string .


###  http://example.org/ontologies/atkis#long
:long rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#modellart
:modellart rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:comment "Model type identifier" .


###  http://example.org/ontologies/atkis#municipality
:municipality rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#objart
:objart rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:comment "Object type" .


###  http://example.org/ontologies/atkis#objart_txt
:objart_txt rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Settlement ;
            rdfs:comment "Object type text" .


###  http://example.org/ontologies/atkis#objart_z
:objart_z rdf:type owl:DatatypeProperty ;
          rdfs:comment "Object types without spatial references (ZUSO)" .


###  http://example.org/ontologies/atkis#objectId
:objectId rdf:type owl:DatatypeProperty ;
          rdfs:domain :Settlement ,
                      :Traffic .


###  http://example.org/ontologies/atkis#objecttxt
:objecttxt rdf:type owl:DatatypeProperty ;
           rdfs:domain :SpecialFacility ;
           rdfs:range xsd:string .


###  http://example.org/ontologies/atkis#objid
:objid rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:comment "Unique Object identifier" .


###  http://example.org/ontologies/atkis#objid_z
:objid_z rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:comment "Unique identifier for feature types without spatial reference (ZUSO)" .


###  http://example.org/ontologies/atkis#place
:place rdf:type owl:DatatypeProperty .


###  http://example.org/ontologies/atkis#rgs
:rgs rdf:type owl:DatatypeProperty ;
     rdfs:domain :Settlement .


###  http://example.org/ontologies/atkis#wdm
:wdm rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Dedication" .


###  http://example.org/ontologies/atkis#znm
:znm rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Middle name" .


###  http://example.org/ontologies/atkis#zus
:zus rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:comment "Condition" .


###  http://www.opengis.net/ont/geosparql#asWKT
geo:asWKT rdf:type owl:DatatypeProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#geometry
rdf:geometry rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://example.org/ontologies/atkis#Agricultural
:Agricultural rdf:type owl:Class ;
              rdfs:subClassOf :Vegetation .


###  http://example.org/ontologies/atkis#AreaDominatedByBuilding
:AreaDominatedByBuilding rdf:type owl:Class ;
                         rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#BuildingFacility
:BuildingFacility rdf:type owl:Class ;
                  rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#Forestry
:Forestry rdf:type owl:Class ;
          rdfs:subClassOf :Vegetation .


###  http://example.org/ontologies/atkis#GN250
:GN250 rdf:type owl:Class .


###  http://example.org/ontologies/atkis#Geographical
:Geographical rdf:type owl:Class ;
              rdfs:subClassOf schema:Place .


###  http://example.org/ontologies/atkis#Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#MeasurementData
:MeasurementData rdf:type owl:Class ;
                 rdfs:subClassOf :Relief .


###  http://example.org/ontologies/atkis#Path
:Path rdf:type owl:Class ;
      rdfs:subClassOf :Traffic .


###  http://example.org/ontologies/atkis#Protected
:Protected rdf:type owl:Class ;
           rdfs:subClassOf schema:Place .


###  http://example.org/ontologies/atkis#RailwayTraffic
:RailwayTraffic rdf:type owl:Class ;
                rdfs:subClassOf :Traffic .


###  http://example.org/ontologies/atkis#Relation
:Relation rdf:type owl:Class ;
          rdfs:subClassOf :VerticalRelation .


###  http://example.org/ontologies/atkis#Relief
:Relief rdf:type owl:Class ;
        rdfs:comment "Mountain elevation, clips" .


###  http://example.org/ontologies/atkis#ReliefForm
:ReliefForm rdf:type owl:Class ;
            rdfs:subClassOf :Relief .


###  http://example.org/ontologies/atkis#RoadTraffic
:RoadTraffic rdf:type owl:Class ;
             rdfs:subClassOf :Traffic .


###  http://example.org/ontologies/atkis#Settlement
:Settlement rdf:type owl:Class ;
            rdfs:comment "Settlements are towns." .


###  http://example.org/ontologies/atkis#SpecialFacility
:SpecialFacility rdf:type owl:Class ;
                 rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#Traffic
:Traffic rdf:type owl:Class ;
         rdfs:comment "Ranging from pathways, air, shipping, railway traffic and transporting facilities" .


###  http://example.org/ontologies/atkis#TransportBuildingAndFacility
:TransportBuildingAndFacility rdf:type owl:Class ;
                              rdfs:subClassOf :Traffic .


###  http://example.org/ontologies/atkis#VegChar
:VegChar rdf:type owl:Class ;
         rdfs:subClassOf :Vegetation .


###  http://example.org/ontologies/atkis#Vegetation
:Vegetation rdf:type owl:Class ;
            rdfs:subClassOf schema:Place ;
            rdfs:comment "agricultural areas" .


###  http://example.org/ontologies/atkis#VegetationArea
:VegetationArea rdf:type owl:Class ;
                rdfs:subClassOf :Vegetation .


###  http://example.org/ontologies/atkis#VerticalRelation
:VerticalRelation rdf:type owl:Class ;
                  rdfs:comment "what feature types has a vertical relation" .


###  http://example.org/ontologies/atkis#WaterBodyChar
:WaterBodyChar rdf:type owl:Class ;
               rdfs:subClassOf schema:BodyOfWater .


###  http://example.org/ontologies/atkis#WaterLineAxis
:WaterLineAxis rdf:type owl:Class ;
               rdfs:subClassOf schema:BodyOfWater .


###  http://schema.org/AdministrativeArea
schema:AdministrativeArea rdf:type owl:Class ;
                          rdfs:subClassOf schema:Place ;
                          rdfs:comment "A geographical region, typically under the jurisdiction of a particular government." .


###  http://schema.org/Airport
schema:Airport rdf:type owl:Class ;
               rdfs:subClassOf schema:CivicStructure ;
               rdfs:comment "An airport."@en .


###  http://schema.org/BoatTrip
schema:BoatTrip rdf:type owl:Class ;
                rdfs:subClassOf schema:Trip .


###  http://schema.org/BodyOfWater
schema:BodyOfWater rdf:type owl:Class ;
                   rdfs:subClassOf schema:Landform ;
                   rdfs:comment "A body of water, such as a sea, ocean, or lake." .


###  http://schema.org/City
schema:City rdf:type owl:Class ;
            rdfs:subClassOf schema:AdministrativeArea ;
            rdfs:comment "A city or town." .


###  http://schema.org/CivicStructure
schema:CivicStructure rdf:type owl:Class ;
                      rdfs:subClassOf schema:Place ;
                      rdfs:comment "A public structure, such as a town hall or concert hall." .


###  http://schema.org/Intangible
schema:Intangible rdf:type owl:Class .


###  http://schema.org/Landform
schema:Landform rdf:type owl:Class ;
                rdfs:subClassOf schema:Place ;
                rdfs:comment "A landform or physical feature. Landform elements include mountains, plains, lakes, rivers, seascape and oceanic waterbody interface features such as bays, peninsulas, seas and so forth, including sub-aqueous terrain features such as submersed mountain ranges, volcanoes, and the great ocean basins." .


###  http://schema.org/LandmarkOrHistoricalBuilding
schema:LandmarkOrHistoricalBuilding rdf:type owl:Class ;
                                    rdfs:subClassOf schema:Place .


###  http://schema.org/Place
schema:Place rdf:type owl:Class ;
             rdfs:comment "Entities that have a somewhat fixed, physical extension."@en .


###  http://schema.org/RiverBodyOfWater
schema:RiverBodyOfWater rdf:type owl:Class ;
                        rdfs:subClassOf schema:BodyOfWater ;
                        rdfs:comment "A river (for example, the broad majestic Shannon)." .


###  http://schema.org/TrainTrip
schema:TrainTrip rdf:type owl:Class ;
                 rdfs:subClassOf schema:Trip .


###  http://schema.org/Trip
schema:Trip rdf:type owl:Class ;
            rdfs:subClassOf schema:Intangible .


###  http://www.opengis.net/ont/geosparql#geometry
geo:geometry rdf:type owl:Class .


###  http://www.w3.org/2003/01/geo/wgs84_pos#Point
wgs:Point rdf:type owl:Class ;
          rdfs:subClassOf wgs:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
wgs:SpatialThing rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#
wgs: rdf:type owl:NamedIndividual ;
     rdfs:label "geo" ;
     <http://purl.org/dc/elements/1.1/date> "$Date: 2009/04/20 15:00:30 $" ;
     <http://purl.org/dc/elements/1.1/description> """A vocabulary for representing latitude, longitude and 
 altitude information in the WGS84 geodetic reference datum. 
 Version $Id: wgs84_pos.rdf,v 1.22 2009/04/20 15:00:30 timbl Exp $. See http://www.w3.org/2003/01/geo/ for more details.""" ;
     <http://purl.org/dc/elements/1.1/title> "WGS84 Geo Positioning: an RDF vocabulary" ;
     rdfs:comment """
