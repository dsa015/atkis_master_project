[PrefixDeclaration]
:		http://example.org/ontologies/atkis#
geo:		http://www.opengis.net/ont/geosparql#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
uom:		http://www.opengis.net/def/uom/OGC/1.0/
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
geof:		http://www.opengis.net/def/function/geosparql/
rdfs:		http://www.w3.org/2000/01/rdf-schema#
schema:		http://schema.org/

[MappingDeclaration] @collection [[
mappingId	Area for linestring_l, geographical areas
target		:Administrative/l/{gid} a :Administrative ; geo:hasDefaultGeometry :Administrative/geo/l/{gid} . :Administrative/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, st_astext(geom) as wkt from "geb01_l"

mappingId	administrative polygon ?
target		:Administrative/f/{gid} a :Administrative ; rdfs:label {bez_lan}@de ; geo:hasDefaultGeometry :Administrative/geo/f/{gid} . :Administrative/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, bez_lan, st_astext(geom) as wkt from "geb01_f"

mappingId	Area, geographical areas polygon (f)
target		:Geographical/f/{gid} a :Geographical ; rdfs:label {nam} ; geo:hasDefaultGeometry :Geographical/geo/f/{gid} . :Geographical/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "geb02_f"

mappingId	Protected areas polygons_f
target		:Protected/f/{gid} a :Protected ; rdfs:label {nam} ; geo:hasDefaultGeometry :Protected/geo/f{gid} . :Protected/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "geb03_f"

mappingId	WaterBody polygon_f
target		:WaterBodyCourse/f/{id} a :WaterBodyCourse ; rdfs:label {nam} ; :objart_txt {objart_txt} ; geo:hasDefaultGeometry :WaterBodyCourse/geo/f/{id} . :WaterBodyCourse/geo/f/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, objart_txt, st_astext(st_transform(geom,4326)) as wkt from "gew01_f"

mappingId	WaterBody linestrings_l
target		:WaterBodyCourse/l/{gid} a :WaterBodyCourse ; rdfs:label {nam} ; geo:hasDefaultGeometry :WaterBodyCourse/geo/l/{gid} . :WaterBodyCourse/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(st_transform(geom,4326)) as wkt from "gew01_l"

mappingId	WaterBodyChar polygon_f
target		:WaterBodyChar/f/{gid} a :WaterBodyChar ; rdfs:label {nam} ; geo:hasDefaultGeometry :WaterBodyChar/geo/f/{gid} . :WaterBodyChar/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "gew02_f"

mappingId	WaterBodyChar point_p
target		:WaterBodyChar/p/{gid} a :WaterBodyChar ; rdfs:label {nam} ; geo:hasDefaultGeometry :WaterBodyChar/geo/p/{gid} . :WaterBodyChar/geo/p/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "gew02_p"

mappingId	WaterLineAxis linestring_l
target		:WaterLineAxis/l/{gid} a :WaterLineAxis ; rdfs:label {nam} ; geo:hasDefaultGeometry :WaterLineAxis/geo/l/{gid} . :WaterLineAxis/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "gew03_l"

mappingId	VerticalRelation- no geom, relation between feature in traffic(ver)
target		:VerticalRelation/{gid} a :VerticalRelation . 
source		select gid, ebene_1, ebene_2 from "hdu01_b"

mappingId	Relief linestring_l
target		:Relief/l/{id} a :Relief ; rdfs:label {nam} ; geo:hasDefaultGeometry :Relief/geo/l/{id} . :Relief/geo/l/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, st_astext(st_transform(geom, 4326)) as wkt from "rel01_l"

mappingId	Relief point_p
target		:Relief/p/{gid} a :Relief ; rdfs:label {nam} ; geo:hasDefaultGeometry :Relief/geo/p/{gid} . :Relief/geo/p/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "rel01_p"

mappingId	Relief MeasurementData point_p
target		:MeasurementData/p/{id} a :MeasurementData ; geo:hasDefaultGeometry :MeasurementData/geo/p/{id} . :MeasurementData/geo/p/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, st_astext(st_transform(geom, 4326)) as wkt from rel02_p

mappingId	Settlement, named location polygon_f
target		:Location/f/{gid} a :Location ; rdfs:label {nam} ; geo:hasDefaultGeometry :Location/geo/f/{gid} . :Location/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(st_transform(geom, 4326)) as wkt from "sie01_f"

mappingId	Settlement, named location point_p
target		:Location/p/{gid} a :Location ; rdfs:label {nam} ; geo:hasDefaultGeometry :Location/geo/p/{gid} . :Location/geo/p/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(st_transform(geom,4326)) as wkt from "sie01_p"

mappingId	areas dominated by buildings, polygon f
target		:Areas_d_b/f/{gid} a :AreaDominatedByBuilding ; :objart {objart} ; rdfs:label {nam} ; :fkt {fkt} ; geo:hasDefaultGeometry :Areas_d_b/geo/f/{gid} . :Areas_d_b/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, fkt, objart, st_astext(st_transform(geom,4326)) as wkt from "sie02_f"

mappingId	Building/facilityes polygon_f
target		:BuildingFacility/f/{id} a :BuildingFacility ; rdfs:label {nam} ; geo:hasDefaultGeometry :BuildingFacility/geo/f/{id} . :BuildingFacility/geo/f/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, bwf, st_astext(geom) as wkt from "sie03_f"

mappingId	Building/facilityes linestring_l
target		:BuildingFacility/l/{gid} a :BuildingFacility ; geo:hasDefaultGeometry :BuildingFacility/geo/l/{gid} . :BuildingFacility/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, st_astext(geom) as wkt from "sie03_l"

mappingId	Building/facilityes point_p
target		:BuildingFacility/p/{id} a :BuildingFacility ; rdfs:label {nam} ; geo:hasMetricLength {hho} ; geo:hasDefaultGeometry :BuildingFacility/geo/p/{id} . :BuildingFacility/geo/p/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, hho, bwf, st_astext(st_transform(geom, 4326)) as wkt from "sie03_p"

mappingId	SpecialFacility polygon_f
target		:SpecialFacility/f/{gid} a :SpecialFacility ; rdfs:label {nam} ; :objart_txt {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/f/{gid} . :SpecialFacilitiy/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(geom) as wkt, objart_txt from "sie04_f"

mappingId	SpecialFacility linestring_l
target		:SpecialFacility/l/{gid} a :SpecialFacility ; rdfs:label {nam} ; :objart_txt {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/l/{gid} . :SpecialFacilitiy/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(geom) as wkt, objart_txt from "sie04_l"

mappingId	SpecialFacility point_p
target		:SpecialFacility/p/{gid} a :SpecialFacility ; rdfs:label {nam} ; :objart_txt {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/p/{gid} . :SpecialFacilitiy/geo/p/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral ; :objart_txt {objart_txt} . 
source		select gid, nam, st_astext(geom) as wkt, objart_txt from "sie04_p"

mappingId	Building point_p
target		:Building/p/{id} a :Building ; rdfs:label {nam} ; :gfk {gfk} ; geo:hasDefaultGeometry :Building/geo/p/{id} . :Building/geo/p/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, bwf, gfk ,st_astext(st_transform(geom, 4326)) as wkt from "sie05_p"

mappingId	Agricultural polygon_f
target		:Agricultural/f/{gid} a :Agricultural ; rdfs:label {nam} ; geo:hasDefaultGeometry :Agricultural/geo/f/{gid} . :Agricultural/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(st_transform(geom,4326)) as wkt from "veg01_f"

mappingId	Forestry polygon_f
target		:Forestry/f/{gid} a :Forestry ; rdfs:label {nam} ; geo:hasDefaultGeometry :Forestry/geo/f/{gid} . :Forestry/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(geom) as wkt from "veg02_f"

mappingId	VegetationArea polygon_f
target		:VegetationArea/f/{gid} a :VegetationArea ; rdfs:label {nam} ; geo:hasDefaultGeometry :Vegetation/geo/f/{gid} . :Vegetation/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(st_transform(geom, 4326)) as wkt from "veg03_f"

mappingId	vegetation_characteristics polygon_f
target		:VegChar/f/{gid} a :VegChar ; rdfs:label {nam} ; geo:hasDefaultGeometry :VegChar/geo/f/{gid} . :VegChar/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(geom) as wkt from "veg04_f"

mappingId	Road traffic linestring_l
target		:RoadTraffic/l/{id} a :RoadTraffic ; rdfs:label {bez} ; :znm {znm} ; geo:hasDefaultGeometry :RoadTraffic/geo/l/{id} . :RoadTraffic/geo/l/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, bez, znm, st_astext(st_transform(geom,4326)) as wkt from "ver01_l"

mappingId	Road paths linestring_l
target		:Paths/l/{gid} a :Path ; rdfs:label {nam} ; geo:hasMetricLength {length}^^xsd:double ; geo:hasDefaultGeometry :Paths/geo/l/{gid} . :Paths/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(st_transform(geom, 4326)) as wkt, st_length(geom) as length from "ver02_l"

mappingId	RailwayTraffic polygon_f
target		:RailwayTraffic/f/{gid} a :Path ; geo:hasDefaultGeometry :RailwayTraffic/geo/f/{gid} . :RailwayTraffic/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid ,st_astext(geom) as wkt from "ver03_f"

mappingId	RailwayTraffic linestring_l
target		:RailwayTraffic/l/{gid} a :RailwayTraffic ; rdfs:label {nam} ; geo:hasDefaultGeometry :RailwayTraffic/geo/f/{gid} . :RailwayTraffic/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam ,st_astext(geom) as wkt from "ver03_l"

mappingId	AirTraffic polygon_f
target		:AirTraffic/f/{gid} a :AirTraffic ; rdfs:label {nam} ; :bez {bez} ; geo:hasDefaultGeometry :AirTraffic/geo/f/{gid} . :AirTraffic/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, bez ,st_astext(geom) as wkt from "ver04_f"

mappingId	ShippingTraffic linestring_l
target		:ShippingTraffic/l/{gid} a :ShippingTraffic ; rdfs:label {nam} ; geo:hasDefaultGeometry :ShippingTraffic/geo/l/{gid} . :ShippingTraffic/geo/l/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(st_transform(geom, 4326)) as wkt from "ver05_l"

mappingId	Transport_b_f polygon_f
target		:Transport_b_f/f/{gid} a :TransportBuildingAndFacility ; rdfs:label {nam} ; geo:hasDefaultGeometry :Transport_b_f/geo/f/{gid} . :Transport_b_f/geo/f/{gid} a geo:Geometry , <http://www.opengis.net/ont/sf#Polygon> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select gid, nam, st_astext(geom) as wkt from "ver06_f"

mappingId	Transport_b_f linestring_l
target		:Transport_b_f/l/{id} a :TransportBuildingAndFacility ; rdfs:label {nam} ; :bez {bez} ; geo:hasDefaultGeometry :Transport_b_f/geo/l/{id} . :Transport_b_f/geo/l/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#LineString> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, bez ,st_astext(geom) as wkt from "ver06_l"

mappingId	Transport_b_f point_p
target		:Transport_b_f/p/{id} a :TransportBuildingAndFacility ; rdfs:label {nam} ; :bez {bez} ; geo:hasDefaultGeometry :Transport_b_f/geo/p/{id} . :Transport_b_f/geo/p/{id} a geo:Geometry , <http://www.opengis.net/ont/sf#Point> ; geo:asWKT {wkt}^^geo:wktLiteral . 
source		select id, nam, bez, st_astext(geom) as wkt from "ver06_p"

mappingId	Transport_b_f polygon and point?
target		:Transport_b_f/f/{gid} a :TransportBuildingAndFacility ; rdfs:label {f_name} , {p_name} ; :hasTraffic :Transport_b_f/p/{id} . 
source		SELECT ver06_f.gid AS gid, ver06_f.nam AS f_name, ver06_p.id AS id, ver06_p.nam AS p_name FROM "ver06_f" INNER JOIN "ver06_p" ON ver06_f.gid = ver06_p.id;

mappingId	obj settlement, see spatially within obj areas (admin districts)
target		:GN250/{id} a :GN250 ; rdfs:label {kreis}^^xsd:string ; :area {bundesland}^^xsd:string ; geo:sfWithin :Settlement/f/{gid} . 
source		select * from "GN250_b"
			INNER JOIN sie01_f
			ON "GN250_b".name = sie01_f.nam

mappingId	sie02 inside settlements
target		:Location/f/{gid1} geo:sfContains :Areas_d_b/f/{gid2} . 
source		SELECT h.gid as gid1, m.gid as gid2
			FROM sie01_f h join sie02_f m
			on st_contains(h.geom, m.geom)

mappingId	building facility in municipality
target		:BuildingFacility/f/{id} geo:ehInside :Location/f/{gid} . 
source		select a.gid, b.id from
			sie01_f a join sie03_f b on st_within(b.geom, a.geom)

mappingId	special facility within a municipality
target		:SpecialFacility/f/{gid} geo:ehInside :Location/f/{gid2} . 
source		select a.gid as gid2, b.gid from
			sie01_f a join sie04_f b on st_contains(a.geom, b.geom)

mappingId	settlement f inside admin areaas
target		:Location/f/{gid} geo:ehInside :Administrative/f/{gid2} . 
source		select * from city_in_area

mappingId	building inside settlement
target		:Building/p/{id} geo:ehInside :Location/f/{gid} . 
source		select * from building_in_city

mappingId	river through settlement
target		:WaterBodyCourse/f/{id} geo:sfIntersects :Location/f/{gid} . 
source		select * from wbcourse_in_loc

mappingId	WaterBody linestrings_length
target		:WaterBodyCourse/geo/l/{gid} geo:hasMetricLength {length}^^xsd:double . 
source		select gid, st_length(geom) as length from "gew01_l"

mappingId	roadtraffic across sie
target		:RoadTraffic/l/{id} geo:sfCrosses :Location/f/{gid} . 
source		select a.id, b.gid from ver01_l a join sie01_f b on st_intersects(a.geom, b.geom)

mappingId	waterbody through vegetation
target		:Agricultural/f/{gid2} a :Agricultural ; geo:sfContains :WaterBodyCourse/l/{gid1} . 
source		select a.gid as gid1, b.gid as gid2 from gew01_l a
			join veg01_f b on st_contains(b.geom, a.geom)

mappingId	traffic connected to areas
target		:Paths/l/{gid1} geo:sfContains :Location/f/{gid2} . 
source		select a.gid as gid1, b.gid as gid2 from ver02_l a
			join sie01_f b on st_contains(b.geom, a.geom)
]]

